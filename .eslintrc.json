{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "import/prefer-default-export": "off",
        "indent": "off",
        "import/extensions": "off",
        "no-case-declarations": "off",
        "no-plusplus": "off",
        "guard-for-in":"off",
        "no-underscore-dangle":"off"
    }
}